{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tarosky/event-date",
	"version": "1.0.0",
	"title": "イベント日",
	"category": "tarosky",
	"icon": "calendar-alt",
	"description": "投稿に設定されたイベント日（カスタムフィールド）を表示します。値はメタボックスまたはブロック内で編集できます。",
	"keywords": [ "イベント", "日付", "カスタムフィールド", "meta" ],
	"textdomain": "headfirst-create-block",
	"attributes": {
		"prefix": {
			"type": "string",
			"default": "開催日："
		}
	},
	"usesContext": [ "postId", "postType" ],
	"supports": {
		"html": false,
		"align": false,
		"typography": {
			"fontSize": true,
			"lineHeight": true
		},
		"color": {
			"text": true,
			"background": true
		},
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}