{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "tarosky/mm-alert",
	"version": "0.1.0",
	"title": "mm-アラート",
	"category": "morimoto",
	"icon": "lightbulb",
	"description": "森本が作成したアラートブロック",
	"keywords": ["alert", "アラート", "morimoto"],
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "tarosky",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": ".mm-alert",
			"default": ""
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"styles": [
		{
			"name": "warning",
			"label": "Warning（黄）",
			"isDefault": true
		},
		{
			"name": "info",
			"label": "Info（緑）"
		},
		{
			"name": "danger",
			"label": "Danger（赤）"
		}
	],
	"viewScript": "file:./view.js"
}
